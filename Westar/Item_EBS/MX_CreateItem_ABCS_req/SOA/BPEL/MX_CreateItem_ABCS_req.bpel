<process name="MX_CreateItem_ABCS_req"
         targetNamespace="http://xmlns.oracle.com/Item_EBS/MX_CreateItem_ABCS_req/MX_CreateItem_ABCS_req"
         xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         xmlns:client="http://xmlns.oracle.com/Item_EBS/MX_CreateItem_ABCS_req/MX_CreateItem_ABCS_req"
         xmlns:ora="http://schemas.oracle.com/xpath/extension"
         xmlns:ui="http://xmlns.oracle.com/soa/designer"
         xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
         xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         xmlns:ns1="http://xmlns.westarenergy.com/WSDL/MX_CreateItem_ABCS_req/V1.0"
         xmlns:ns2="http://xmlns.westarenergy.com/EBM/CreateItem/V1.0"
         xmlns:ns3="http://xmlns.westarenergy.com/Common/Logging/V1.0"
         xmlns:xp20="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.Xpath20"
         xmlns:bpws="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:oraext="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.ExtFunc"
         xmlns:dvm="http://www.oracle.com/XSL/Transform/java/oracle.tip.dvm.LookupValue"
         xmlns:hwf="http://xmlns.oracle.com/bpel/workflow/xpath"
         xmlns:ids="http://xmlns.oracle.com/bpel/services/IdentityService/xpath"
         xmlns:bpm="http://xmlns.oracle.com/bpmn20/extensions" xmlns:ess="http://xmlns.oracle.com/scheduler"
         xmlns:xdk="http://schemas.oracle.com/bpel/extension/xpath/function/xdk"
         xmlns:xref="http://www.oracle.com/XSL/Transform/java/oracle.tip.xref.xpath.XRefXPathFunctions"
         xmlns:ldap="http://schemas.oracle.com/xpath/extension/ldap"
         xmlns:ns4="http://xmlns.westarenergy.com/core/CommonTypes/V1.0"
         xmlns:ns5="http://xmlns.westarenergy.com/Common/Errors/V1.0"
         xmlns:ns6="http://schemas.oracle.com/events/edl/LogEvent"
         xmlns:ns8="http://schemas.xmlsoap.org/ws/2003/03/addressing"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:ns7="http://xmlns.westarenergy.com/EBM/CommonEBMHeader/V1.0"
         xmlns:ns9="http://xmlns.westarenergy.com/WSDL/Item_EBS/V1.0"
         xmlns:ns10="http://xmlns.westarenergy.com/EBO/Item/V1.0">
    <import namespace="http://xmlns.westarenergy.com/WSDL/Item_EBS/V1.0" location="../WSDLs/Item_EBS.wsdl"
            importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://xmlns.westarenergy.com/Common/Logging/V1.0"
            location="oramds:/apps/WestarEnergy/Events/xsd/LogEvent_V1.0.xsd"
            importType="http://www.w3.org/2001/XMLSchema"/>
    <import namespace="http://xmlns.westarenergy.com/WSDL/MX_CreateItem_ABCS_req/V1.0"
            location="oramds:/apps/WestarEnergy/ABCS/wsdl/MX_CreateItem_ABCS_req.wsdl"
            importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://xmlns.westarenergy.com/WSDL/MX_CreateItem_ABCS_req/V1.0"
            location="../WSDLs/MX_CreateItem_ABCS_reqWrapper.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"
            ui:processWSDL="true"/>
    <import namespace="http://xmlns.westarenergy.com/EBM/CreateItem/V1.0"
            location="oramds:/apps/WestarEnergy/EBM/CreateItem_EBM_V1.0.xsd"
            importType="http://www.w3.org/2001/XMLSchema"/>
    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
        PARTNERLINKS                                                      
        List of services participating in this BPEL process               
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    --> 
    <partnerLinks>
        <!-- 
      The 'client' role represents the requester of this service. It is 
      used for callback. The location and correlation information associated
      with the client role are automatically set using WS-Addressing.
    -->
        <partnerLink name="mx_createitem_abcs_req_client_ep" partnerLinkType="ns1:mx_createitem_abcs_req_client_ep"
                     myRole="ItemCRUDPortType"/>
        <partnerLink name="Item_EBS_ref" partnerLinkType="ns9:Item_EBS_ref" partnerRole="ItemCRUDPortType"/>
    </partnerLinks>

    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
        VARIABLES                                                        
        List of messages and XML documents used within this BPEL process 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    -->
    <variables>
        <!-- Reference to the message passed as input during initiation -->

        <!-- Reference to the message that will be sent back to the requester during callback -->
        <variable name="inputVariable" messageType="ns1:CreateItemRequestMessage"/>
        <variable name="outputVariable" messageType="ns1:CreateItemResponseMessage"/>
        <variable name="LogEvent" element="ns3:LogEvent"/>
        <variable name="ReplyError_CreateItem_OutputVariable" messageType="ns1:CreateItemResponseMessage"/>
        <variable name="Invoke_CreateItem_InputVariable" messageType="ns9:CreateItemRequestMessage"/>
        <variable name="Invoke_CreateItem_OutputVariable" messageType="ns9:CreateItemResponseMessage"/>
    </variables>
    <faultHandlers>
        <catchAll>
            <sequence name="Sequence1">
                <assign name="Xform_ErrorInfo_LogEvent">
                    <bpelx:annotation>
                        <bpelx:pattern patternName="bpelx:transformation"></bpelx:pattern>
                    </bpelx:annotation>
                    <copy>
                        <from>ora:doXSLTransformForDoc("../Transformations/Xform_ErrorInfo_LogEvent.xsl", $inputVariable.payload)</from>
                        <to variable="LogEvent"/>
                    </copy>
                </assign>
                <assign name="AssignLogEvent">
                    <copy>
                        <from>concat(ora:getFaultName(), ' at ', ora:getCompositeName(), ':', ora:getComponentName())</from>
                        <to>$LogEvent/ns3:Message</to>
                    </copy>
                    <copy>
                        <from>ora:getFaultName()</from>
                        <to>$LogEvent/ns3:Errors/ns5:Error/ns5:Name</to>
                    </copy>
                    <copy>
                        <from>ora:getFaultAsString()</from>
                        <to>$LogEvent/ns3:Errors/ns5:Error/ns5:Description</to>
                    </copy>
                    <copy>
                        <from>ora:getComponentInstanceId()</from>
                        <to>$LogEvent/ns3:Errors/ns5:Error/ns5:ComponentInstanceInformation/ns4:InstanceID</to>
                    </copy>
                    <copy>
                        <from>ora:getComponentName()</from>
                        <to>$LogEvent/ns3:Errors/ns5:Error/ns5:ComponentInstanceInformation/ns4:Name</to>
                    </copy>
                    <copy>
                        <from>concat(ora:getCompositeName(), '.', ora:getComponentName(), '.CreateItem')</from>
                        <to>$LogEvent/ns3:Errors/ns5:Error/ns5:ComponentInstanceInformation/ns4:Operation</to>
                    </copy>
                    <copy>
                        <from>ora:getCompositeName()</from>
                        <to>$LogEvent/ns3:ComponentInstanceInformation/ns4:Name</to>
                    </copy>
                    <copy>
                        <from>concat(ora:getCompositeName(), ':', ora:getComponentName(), ':CreateItem')</from>
                        <to>$LogEvent/ns3:ComponentInstanceInformation/ns4:Operation</to>
                    </copy>
                    <copy>
                        <from>ora:getFlowId()</from>
                        <to>$LogEvent/ns3:ComponentInstanceInformation/ns4:InstanceID</to>
                    </copy>
                </assign>
                <invoke name="InvokeLogEvent" bpelx:invokeAsDetail="no" bpelx:eventName="ns6:LogEvent"
                        inputVariable="LogEvent"/>
                <assign name="AssignErrorReply">
                    <copy>
                        <from><literal>Failed</literal></from>
                        <to>$ReplyError_CreateItem_OutputVariable.payload/ns2:DataArea/ns2:CreateItemResponseData/ns2:ReturnStatusDetail/ns4:Status</to>
                    </copy>
                    <copy>
                        <from>$LogEvent/ns3:Errors/ns5:Error/ns5:Description</from>
                        <to>$ReplyError_CreateItem_OutputVariable.payload/ns2:DataArea/ns2:CreateItemResponseData/ns2:ReturnStatusDetail/ns4:ErrorDetail/ns4:ErrorMessage</to>
                    </copy>
                    <copy>
                        <from>$LogEvent/ns3:Message</from>
                        <to>$ReplyError_CreateItem_OutputVariable.payload/ns2:DataArea/ns2:CreateItemResponseData/ns2:ReturnStatusDetail/ns4:FormattedMessage</to>
                    </copy>
                </assign>
                <reply name="ReplyError" variable="ReplyError_CreateItem_OutputVariable"
                       partnerLink="mx_createitem_abcs_req_client_ep" portType="ns1:ItemCRUDPortType"
                       operation="CreateItem"/>
            </sequence>
        </catchAll>
    </faultHandlers>
    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
       ORCHESTRATION LOGIC                                               
       Set of activities coordinating the flow of messages across the    
       services integrated within this business process                  
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    -->
    <sequence name="main">
        <!-- Receive input from requestor. (Note: This maps to operation defined in MX_CreateItem_ABCS_req.wsdl) -->
        <receive name="receiveInput" portType="ns1:ItemCRUDPortType" operation="CreateItem"
                 partnerLink="mx_createitem_abcs_req_client_ep" variable="inputVariable"/>
        <assign name="Assign_RcvInput_InvokeItem">
            <copy>
                <from>$inputVariable.payload</from>
                <to>$Invoke_CreateItem_InputVariable.payload</to>
            </copy>
        </assign>
        <invoke name="Invoke_ItemEBS_CreateItem" bpelx:invokeAsDetail="no" partnerLink="Item_EBS_ref"
                portType="ns9:ItemCRUDPortType" operation="CreateItem" inputVariable="Invoke_CreateItem_InputVariable"
                outputVariable="Invoke_CreateItem_OutputVariable"/>
        <assign name="Assign_SROutput_replyOutput">
            <copy>
                <from>$Invoke_CreateItem_OutputVariable.payload</from>
                <to>$outputVariable.payload</to>
            </copy>
        </assign>
        <!-- 
          Asynchronous callback to the requester. (Note: the callback location and correlation id is transparently handled using WS-addressing.)
        -->
        <reply name="replyOutput" portType="ns1:ItemCRUDPortType" operation="CreateItem"
               partnerLink="mx_createitem_abcs_req_client_ep" variable="outputVariable"/>
    </sequence>
</process>