<process name="PPLSFT_CRUD_Item_ABCS_prov"
               targetNamespace="http://xmlns.oracle.com/Item_EBS/PPLSFT_CRUD_Item_ABCS_prov/PPLSFT_CRUD_Item_ABCS_prov"
               xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
               xmlns:client="http://xmlns.oracle.com/Item_EBS/PPLSFT_CRUD_Item_ABCS_prov/PPLSFT_CRUD_Item_ABCS_prov"
               xmlns:ora="http://schemas.oracle.com/xpath/extension"
               xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
               xmlns:ns1="http://xmlns.westarenergy.com/WSDL/PPLSFT_CRUD_Item_ABCS_prov/V1.0"
               xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         xmlns:ui="http://xmlns.oracle.com/soa/designer"
         xmlns:ns2="http://xmlns.oracle.com/Enterprise/FSCM/service/ITEM_SYNC"
         xmlns:ns3="http://xmlns.westarenergy.com/Common/Logging/V1.0"
         xmlns:ns6="http://schemas.xmlsoap.org/ws/2003/03/addressing"
         xmlns:ns7="http://xmlns.westarenergy.com/core/CommonTypes/V1.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:ns5="http://xmlns.westarenergy.com/EBM/CommonEBMHeader/V1.0"
         xmlns:ns4="http://xmlns.westarenergy.com/EBM/CreateItem/V1.0"
         xmlns:xp20="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.Xpath20"
         xmlns:bpws="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:oraext="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.ExtFunc"
         xmlns:dvm="http://www.oracle.com/XSL/Transform/java/oracle.tip.dvm.LookupValue"
         xmlns:hwf="http://xmlns.oracle.com/bpel/workflow/xpath"
         xmlns:ids="http://xmlns.oracle.com/bpel/services/IdentityService/xpath"
         xmlns:bpm="http://xmlns.oracle.com/bpmn20/extensions" 
         xmlns:ess="http://xmlns.oracle.com/scheduler"
         xmlns:xdk="http://schemas.oracle.com/bpel/extension/xpath/function/xdk"
         xmlns:xref="http://www.oracle.com/XSL/Transform/java/oracle.tip.xref.xpath.XRefXPathFunctions"
         xmlns:ldap="http://schemas.oracle.com/xpath/extension/ldap"
         xmlns:ns8="http://xmlns.westarenergy.com/Common/Errors/V1.0"
         xmlns:ns9="http://schemas.oracle.com/events/edl/LogEvent"
         xmlns:ns10="http://xmlns.westarenergy.com/EBO/Item/V1.0"
         xmlns:ns11="http://xmlns.oracle.com/Enterprise/FSCM/schema/ITEM_SYNC.VERSION_1">

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      PARTNERLINKS                                                      
      List of services participating in this BPEL process               
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <import namespace="http://xmlns.westarenergy.com/Common/Logging/V1.0" location="../Schemas/LogEvent_V1.0.xsd"
          importType="http://www.w3.org/2001/XMLSchema"/>
  <import namespace="http://xmlns.westarenergy.com/Common/Logging/V1.0" location="oramds:/apps/WestarEnergy/Events/xsd/LogEvent_V1.0.xsd"
          importType="http://www.w3.org/2001/XMLSchema"/>
  <import namespace="http://xmlns.westarenergy.com/WSDL/PPLSFT_CRUD_Item_ABCS_prov/V1.0"
          location="../WSDLs/PPLSFT_CRUD_Item_ABCS_provWrapper.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"
          ui:processWSDL="true"/>
  <partnerLinks>
    <!-- 
      The 'client' role represents the requester of this service. It is 
      used for callback. The location and correlation information associated
      with the client role are automatically set using WS-Addressing.
    -->
    <partnerLink name="pplsft_crud_item_abcs_prov_client" partnerLinkType="ns1:PPLSFT_CRUD_Item_ABCS_prov" myRole="CreateItemPortType"/>
    <partnerLink name="PPLSFT_ref" partnerLinkType="ns2:ITEM_SYNC_PartnerLinkType" partnerRole="ITEM_SYNC_Provider"/>
  </partnerLinks>

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      VARIABLES                                                        
      List of messages and XML documents used within this BPEL process 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <variables>
    <!-- Reference to the message passed as input during initiation -->
    <variable name="inputVariable" messageType="ns1:CreateItemRequestMessage"/>

    <!-- Reference to the message that will be returned to the requester-->
    <variable name="outputVariable" messageType="ns1:CreateItemResponseMessage"/>
    <variable name="InvokeCreateItem_ITEM_SYNC_InputVariable" messageType="ns2:ITEM_SYNC.VERSION_1"/>
    <variable name="LogEvent2" element="ns3:LogEvent"/>
  </variables>
  <faultHandlers>
    <catchAll>
      <sequence name="Sequence2">
        <assign name="Xform_ErrorInfo_LogEvent">
          <bpelx:annotation>
            <bpelx:pattern patternName="bpelx:transformation"></bpelx:pattern>
          </bpelx:annotation>
          <copy>
            <from>ora:doXSLTransformForDoc("../Transformations/Xform_ErrorInfo_LogEvent.xsl", $inputVariable.payload)</from>
            <to variable="LogEvent2"/>
          </copy>
        </assign>
        <assign name="AssignLogEvent">
          <copy>
            <from>concat(ora:getFaultName(), ' at ', ora:getCompositeName(), ':', ora:getComponentName())</from>
            <to>$LogEvent2/ns3:Message</to>
          </copy>
          <copy>
            <from>concat(ora:getCompositeName(), '.', ora:getComponentName(), '.RequestDesignCUEstimate')</from>
            <to>$LogEvent2/ns3:Errors/ns8:Error/ns8:ComponentInstanceInformation/ns7:Operation</to>
          </copy>
          <copy>
            <from>ora:getCompositeName()</from>
            <to>$LogEvent2/ns3:Errors/ns8:Error/ns8:ComponentInstanceInformation/ns7:Name</to>
          </copy>
          <copy>
            <from>concat(ora:getCompositeName(), ':', ora:getComponentName(), ':RequestDesignCUEstimate')</from>
            <to>$LogEvent2/ns3:ComponentInstanceInformation/ns7:Operation</to>
          </copy>
          <copy>
            <from>ora:getFlowId()</from>
            <to>$LogEvent2/ns3:ComponentInstanceInformation/ns7:InstanceID</to>
          </copy>
          <copy>
            <from>ora:getFaultName()</from>
            <to>$LogEvent2/ns3:Errors/ns8:Error/ns8:Name</to>
          </copy>
          <copy>
            <from>ora:getFaultAsString()</from>
            <to>$LogEvent2/ns3:Errors/ns8:Error/ns8:Description</to>
          </copy>
          <copy>
            <from>ora:getComponentInstanceId()</from>
            <to>$LogEvent2/ns3:Errors/ns8:Error/ns8:ComponentInstanceInformation/ns7:InstanceID</to>
          </copy>
          <copy>
            <from>ora:getComponentName()</from>
            <to>$LogEvent2/ns3:ComponentInstanceInformation/ns7:Name</to>
          </copy>
        </assign>
        <invoke name="Invoke1" bpelx:invokeAsDetail="no" bpelx:eventName="ns9:LogEvent" inputVariable="LogEvent2"/>
        <assign name="AssignErrorReply">
          <copy>
            <from><literal>Failed</literal></from>
            <to>$outputVariable.payload/ns4:DataArea/ns4:CreateItemResponseData/ns4:ReturnStatusDetail/ns7:Status</to>
          </copy>
          <copy>
            <from>$LogEvent2/ns3:Message</from>
            <to>$outputVariable.payload/ns4:DataArea/ns4:CreateItemResponseData/ns4:ReturnStatusDetail/ns7:FormattedMessage</to>
          </copy>
          <copy>
            <from>$LogEvent2/ns3:Errors/ns8:Error/ns8:Description</from>
            <to>$outputVariable.payload/ns4:DataArea/ns4:CreateItemResponseData/ns4:ReturnStatusDetail/ns7:ErrorDetail/ns7:ErrorMessage</to>
          </copy>
        </assign>
        <reply name="ReplyError" portType="ns1:ItemCRUDPortType" operation="CreateItem"
               partnerLink="pplsft_crud_item_abcs_prov_client" variable="outputVariable"/>
      </sequence>
    </catchAll>
  </faultHandlers>
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <sequence name="main">

    <!-- Receive input from requestor. (Note: This maps to operation defined in PPLSFT_CRUD_Item_ABCS_prov.wsdl) -->
    <pick name="Pick1" createInstance="yes">
      <onMessage bpelx:name="CreateItem_OnMessage" portType="ns1:ItemCRUDPortType" operation="CreateItem"
                 partnerLink="pplsft_crud_item_abcs_prov_client" variable="inputVariable">
        <sequence name="Sequence1">
          <assign name="Xform_CreateItemRequest_CreateItem">
            <bpelx:annotation>
              <bpelx:pattern patternName="bpelx:transformation"></bpelx:pattern>
            </bpelx:annotation>
            <copy>
              <from>ora:doXSLTransformForDoc("../Transformations/Xform_CreateItemRequest_CreateItem.xsl", $inputVariable.payload)</from>
              <to variable="InvokeCreateItem_ITEM_SYNC_InputVariable" part="parameter"/>
            </copy>
          </assign>
          <invoke name="InvokeCreateItem" bpelx:invokeAsDetail="no" partnerLink="PPLSFT_ref"
                  portType="ns2:ITEM_SYNC_PortType" operation="ITEM_SYNC"
                  inputVariable="InvokeCreateItem_ITEM_SYNC_InputVariable"/>
          <assign name="Assign_OutputVariable_ResponseMessage">
            <copy>
              <from><literal>success</literal></from>
              <to>$outputVariable.payload/ns4:DataArea/ns4:CreateItemResponseData/ns4:ReturnStatusDetail/ns7:Status</to>
            </copy>
          </assign>
          <reply name="replyOutput" partnerLink="pplsft_crud_item_abcs_prov_client" portType="ns1:ItemCRUDPortType"
                 operation="CreateItem" variable="outputVariable"/>
        </sequence>
      </onMessage>
    </pick>
    <!-- Generate reply to synchronous request -->
  </sequence>
</process>